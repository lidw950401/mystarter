<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <!---Dartifact=org.lidw:mystarter:0.2:war-->
    <!---DremoteRepositories=ali-mymvnrepo::::https://packages.aliyun.com/66c1eb46b9168fe4f613b522/maven/mymvnrepo-->
    <!---DremoteRepositories=github-mystarter1::::https://maven.pkg.github.com/lidw950401/mystarter-->    
    <groupId>org.lidw</groupId>
    <artifactId>mystarter</artifactId>
    <version>0.2-SNAPSHOT</version>
    <packaging>war</packaging>

    <name>mystarter</name>
    <description>
        This is a very simple Jakarta EE application generated by the official Eclipse Starter.
    </description>

    <properties>
        <!--             <altSnapshotDeploymentRepository>
          ali-mymvnrepo::https://packages.aliyun.com/66c1eb46b9168fe4f613b522/maven/mymvnrepo
        </altSnapshotDeploymentRepository>-->
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.report.sourceEncoding>UTF-8</project.report.sourceEncoding>
        <maven.compiler.release>21</maven.compiler.release>
        <jakartaee-api.version>10.0.0</jakartaee-api.version>
        <compiler-plugin.version>3.13.0</compiler-plugin.version>
        <war-plugin.version>3.4.0</war-plugin.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>jakarta.platform</groupId>
            <artifactId>jakarta.jakartaee-api</artifactId>
            <version>${jakartaee-api.version}</version>
            <scope>provided</scope>
        </dependency>
              
    </dependencies>

    <build>
        <finalName>mystarter</finalName>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>${compiler-plugin.version}</version>
            </plugin>
            <plugin>
                <artifactId>maven-war-plugin</artifactId>
                <version>${war-plugin.version}</version>
                <configuration>
                    <failOnMissingWebXml>false</failOnMissingWebXml>
                </configuration>
            </plugin>

        </plugins>
    </build>
        
        
    <distributionManagement>
        <repository>
            <id>github-mystarter1</id>
            <name>GitHub mystarter Apache Maven Packages</name>
            <url>https://maven.pkg.github.com/lidw950401/mystarter</url>
        </repository>
        <!--        <repository>
          <id>ali-mymvnrepo</id>
          <name>ali mystarter Apache Maven Packages</name>
          <url>https://packages.aliyun.com/66c1eb46b9168fe4f613b522/maven/mymvnrepo</url>
        </repository>-->
    </distributionManagement>
</project>
